# top-most EditorConfig file
root = true

# ----------------------------------------------------------------------
# Global defaults for all files
# ----------------------------------------------------------------------
[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
indent_style = space
indent_size = 4
tab_width = 4
max_line_length = 120
force_editorconfig_settings = true
